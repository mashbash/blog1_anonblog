<style type="text/css">
  .container {
    margin: 20px auto;
    width: 600px;
    padding: 0px 20px 20px 20px;
    border: 1px dotted black;
  }
  th {
    text-align: left;
  }
</style>

<div class="container">
  <h1>Welcome to My Blog!</h1>
  <% @posts.each do |post| %>
  <h2><%= post.title %> </h1>
  <p><%= post.content %> </p>
  <% end %>

</div>
